<configuration>
	<appSettings>
		<add key="log4net.Config" value="Log4Net.Config" />
	</appSettings>
	<system.runtime.caching>
		<memoryCache>
			<namedCaches>
				<add name="opensearch" cacheMemoryLimitMegabytes="512" pollingInterval="00:00:30" />
				<add name="iosr-cache" cacheMemoryLimitMegabytes="128" pollingInterval="00:00:30" />
				<add name="mosr-cache" cacheMemoryLimitMegabytes="128" pollingInterval="00:00:30" />
			</namedCaches>
		</memoryCache>
	</system.runtime.caching>
</configuration>