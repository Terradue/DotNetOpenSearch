    <prm:Parameter name="trel" value="{time:relation}" minimum="0" maximum="1">
      <prm:Option label="intersects" value="intersects" />
      <prm:Option label="contains" value="contains" />
      <prm:Option label="during" value="during" />
      <prm:Option label="disjoint" value="disjoint" />
      <prm:Option label="equals" value="equals" />
    </prm:Parameter>
  </Url>
