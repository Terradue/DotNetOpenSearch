    <prm:Parameter name="start" value="{time:start}" minimum="0" maximum="1" maxInclusive="2017-01-23T17:19:01Z" minInclusive="2010-06-01T00:00:57Z"/>
    <prm:Parameter name="end" value="{time:end}" minimum="0" maximum="1" maxInclusive="2017-01-23T18:25:41Z" minInclusive="2010-06-01T01:07:37Z"/>
    <prm:Parameter name="trel" value="{time:relation}" minimum="0" maximum="1">
      <prm:Option label="intersects" value="intersects" />
      <prm:Option label="contains" value="contains" />
      <prm:Option label="during" value="during" />
      <prm:Option label="disjoint" value="disjoint" />
      <prm:Option label="equals" value="equals" />
    </prm:Parameter>
